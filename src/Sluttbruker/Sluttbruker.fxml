            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtFiltrer" layoutX="389.0" layoutY="216.0" promptText="Søk her" />
       <TableView fx:id="tabell1" layoutX="27.0" layoutY="259.0" prefHeight="200.0" prefWidth="350.0">
