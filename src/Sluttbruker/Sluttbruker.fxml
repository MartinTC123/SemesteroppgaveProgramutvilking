      <Button layoutX="45.0" layoutY="468.0" mnemonicParsing="false" onAction="#beregnTotPris" text="Beregn totalpris" />
      <Button layoutX="253.0" layoutY="548.0" mnemonicParsing="false" onAction="#lagreTilFil" text="Lagre datamaskin" />
      <Button layoutX="254.0" layoutY="588.0" mnemonicParsing="false" onAction="#åpneFraFil" text="Åpne datamaskin" />
      <Label fx:id="lblFilbehandling" layoutX="403.0" layoutY="541.0" prefHeight="82.0" prefWidth="310.0" />
