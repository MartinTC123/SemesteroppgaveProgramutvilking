               <TableColumn fx:id="navnC" prefWidth="100.0" text="Navn">
                   <cellValueFactory>
                       <PropertyValueFactory property="navn" />
                   </cellValueFactory>
               </TableColumn>
               <TableColumn fx:id="komponentC" prefWidth="150.0" text="Komponent">
