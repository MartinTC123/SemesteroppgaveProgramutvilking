               <TableColumn fx:id="navnC2" prefWidth="100.0" text="Navn">
                   <cellValueFactory>
                       <PropertyValueFactory property="navn" />
                   </cellValueFactory>
               </TableColumn>
               <TableColumn fx:id="komponentC2" prefWidth="150.0" text="Komponent">
                   <cellValueFactory>
                       <PropertyValueFactory property="komponent" />
                   </cellValueFactory>
               </TableColumn>
               <TableColumn fx:id="prisC2" prefWidth="100.0" text="Pris">
                   <cellValueFactory>
                       <PropertyValueFactory property="pris" />
                   </cellValueFactory>
               </TableColumn>
           </columns>
       </TableView>
      <Label layoutX="443.0" layoutY="465.0" text="Velg en rad og &quot;Click&quot; for å legge til  " />
